--display the database ordered by price. 
SELECT * FROM jewlery
ORDER BY price;

what piece of jewlery is more than $12 ?
SELECT * FROM jewlery WHERE price > 12 ORDER BY price;  

what items do we have the least of ?
SELECT * FROM jewlery ORDER BY quantity;
